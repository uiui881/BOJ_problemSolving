import java.util.Scanner;
import java.util.StringTokenizer;

public class Main {

	public static void main(String[] args) {
		
		String str;
		Scanner sc = new Scanner(System.in);
		str=sc.nextLine();
		StringTokenizer st = new StringTokenizer(str," ");
		
		System.out.println(st.countTokens());
	
	}	
}
