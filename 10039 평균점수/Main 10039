import java.util.Scanner;

public class Main{
	
	public static void main(String[] args){
	
		int[] array = new int[5];
		Scanner sc = new Scanner(System.in);	
		int total=0;
		
		for(int i=0; i<5;i++) {
			array[i]= sc.nextInt();
		}
		
		for(int i=0; i<5;i++) {
			if(array[i]<40) {
				array[i]=40;
			}
			total+=array[i];
		}
		
		System.out.println(total/5);
	}
}
