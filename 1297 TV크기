import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class Main {
	
    public static void main(String args[]) throws Exception {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        StringTokenizer stzr = new StringTokenizer(br.readLine());
        StringBuilder sb = new StringBuilder();
        
        double ac = Double.parseDouble(stzr.nextToken());
        double a = Double.parseDouble(stzr.nextToken());
        double c = Double.parseDouble(stzr.nextToken());
        
        double x = Math.sqrt(Math.pow(ac, 2)/(Math.pow(a, 2)+Math.pow(c, 2)));
        
        int result1 = (int) ((int) a*x);
        int result2 = (int) ((int) c*x);
       
        System.out.println(result1+" "+result2);
    }
}
